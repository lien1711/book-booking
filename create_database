create database library_db2 character set utf8;

use library_db2;

create table books (
	book_id integer not null primary key,
	book_name char(100),
	author char(100)
);

create table users (
	user_id integer not null primary key,
	user_name char(100)
);

create table lending (
	lending_id integer not null primary key auto_increment,
	user_id integer,
	book_id integer
);



